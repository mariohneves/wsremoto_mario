package br.com.bankofoz.implementar;

import br.com.bankofoz.magic.Magic;
import br.com.bankofoz.modelo.Conta;
import br.com.bankofoz.modelo.Cliente;
import br.com.bankofoz.modelo.Endereco;

public class ImplementarConta {

	public static void main(String[] args) {

	
		Conta conta = new Conta(
				Magic.s("Número"),
				Magic.b("Digito"),
				Magic.sho("Agência"),
				Magic.f("Saldo"),
				new Cliente(
						Magic.i("ID"),
						Magic.s("Nome"),
						Magic.s("Email"),
						Magic.b("Especial"),
						Magic.s("CPF")	
								),
								new Endereco(
										Magic.s("Logradouro"),
										Magic.s("Número"),
										Magic.s("Complemento"),
										Magic.s("Bairro"),
										Magic.s("Cidade"),
										Magic.s("UF")
										)
						);
				
		
						
		System.out.println(conta.toString());
		
		
	}
}
