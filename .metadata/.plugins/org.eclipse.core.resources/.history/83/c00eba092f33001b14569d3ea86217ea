package variaveis;

public class TiposDados {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// dados númericos: tipo de dado que PODE ser utilizado em uma expressão aritimetica, ou quando for um dado sensivel para busca (grau de importância)
		/* No Java representado pelo :
		 * 		int -> se número inteiro
		 * 		double -> números reais (casa decimal é importante)
		 * CEP no contexto correios
		 * CPF no contexto receita federal (sensibilidade e necessidade critica do dado)
		 * 
		 * 
		 * Dados alfa-númericos: não será utilizado para cálculos
		 * Em java é representado pela String (nesse caso necessário criar entre "")
		 * CEP = no contexto DP do Itau
		 * CPF no contexto cadastro faculdade
		 */
		
		//criar variavel ==> <tipo de dado> <nome da variavel> = <valor>
		String nome = "Mario Neves";
		int idade = 28;
		double altura = 1.70;
		double peso = 65;
		System.out.println("Nome..: " + nome);
		System.out.println("Idade.: " + idade);
		System.out.println("Altura: " + altura);
		System.out.println("Peso..: " + peso);
		
				
		
				
		
		

		
		
		
		
		
		
		
		
		
	}

}
